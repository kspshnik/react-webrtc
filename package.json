  "license": "MIT",
  "private": true,
  "dependencies": {
    "concurrently": "^7.0.0"
  },
  "scripts": {
    "dev": "concurrently \"yarn --cwd server dev\" \"yarn --cwd client dev\"",
    "start:client": "cd client && npm run build",
    "start:server": "cd server && node index.js",
    "start": "npm run start:client && npm run start:server"
  }
